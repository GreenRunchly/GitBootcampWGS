<dialog id="classParticipantShow" class="wide-med">
    <div class="panel-back aksiModal" value="close"></div>
    <div class="content">
        <div class="main">
            <header class="banner small">
                <div class="title">
                    <h1><i class="fas fa-users"></i>Partisipan</h1>
                    <p>Terdapat <span class="pageParticipantCurrent">...</span> per <span class="pageParticipantMax">...</span> partisipan yang bergabung!</p>
                </div>
            </header>
            <div class="blok-list flex">
                <div class="item-group">
                    <label><b style="font-size: 1.3rem;">Cari Nama Participan</b></label>
                    <input type="text" class="searchInput" area="participantListing" placeholder="Ketik disini untuk mencari...">
                </div>
            </div>
            <div class="blok-list participantListing">
                <a class="banner hoverable small list-item ghost">
                    <div class="title">
                        <h3><i class="far fa-question"></i>Tidak ada..</h3>
                        <p>Belum ada participant lainnya.</p>
                    </div>
                </a>
            </div>
        </div>
        <div class="options">
            <div class="blok-list flex">
                <button class="button center aksiModal" value="batal">
                    <div class="icon"><i class="far fa-times"></i></div>
                    <div class="text"><span>Tutup</span></div>
                </button>
            </div>
        </div>
    </div>
    <script>
        // Handle response masuk modal
        modalResponse('classParticipantShow', (modalid) =>{
            
        }, (modalid) =>{
            async function lakukan() {
                // Nothing
            }; lakukan(); 
        });
    </script>
</dialog>